<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

    <!-- disable original -->
    <record
        id="web_gantt.assets_backend"
        model="ir.ui.view">
      <field name="active" eval="0" />
    </record>

    <template id="assets_backend"
        name="sitibt web_gantt assets"
        inherit_id="web.assets_backend">

        <xpath expr="." position="inside">
            <link rel="stylesheet"
                  href="/sitibt_web_gantt/static/src/css/gantt.css"/>
            <link rel="stylesheet"
                  href="/sitibt_web_gantt/static/lib/dhtmlxGantt/codebase/dhtmlxgantt.css"/>
            <script type="text/javascript"
                    src="/sitibt_web_gantt/static/lib/dhtmlxGantt/sources/dhtmlxcommon.js"></script>
            <script type="text/javascript"
                    src="/sitibt_web_gantt/static/lib/dhtmlxGantt/sources/dhtmlxgantt_tooltip.js"></script>
            <script type="text/javascript"
                    src="/sitibt_web_gantt/static/src/js/gantt.js"></script>
        </xpath>

    </template>


</data>
</openerp>
